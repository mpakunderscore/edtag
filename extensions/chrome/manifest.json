{
	"manifest_version": 2,

	"name": "PKM Parser",
	"description": "Clever trees.",
	"version": "0.2",

	"options_page": "options.html",
	
    "background": {
      	"scripts": ["background.js", "bolting.js"]
    },
		
	"browser_action": {
		"default_icon": "icons/icon3.png",
		"default_popup": "popup.html"
	},
	  
	"permissions": ["activeTab", "tabs", "http://*/", "https://*/", "notifications"]
}
